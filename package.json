{
  "name": "like",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "zengxingqi",
    "email": "me@zengxingqi.com"
  },
  "description": "An audio and video communication live application",
  "scripts": {
    "lint": "vue-cli-service lint -y",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "publish": "npm run electron:build -- -p always",
    "ico": "png-to-ico build/icons/app@2x.png > build/icons/app.ico",
    "iconset": "sh build/icons/mac-icon.sh",
    "prepare": "husky install"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "electron-updater": "^4.3.9",
    "fast-json-stringify": "^2.7.9",
    "long": "^4.0.0",
    "trtc-electron-sdk": "^8.9.102",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^13.0.0",
    "electron-builder": "^22.13.1",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "png-to-ico": "^2.1.2",
    "prettier": "^2.2.1",
    "sass": "^1.41.1",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "worker-plugin": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  }
}
